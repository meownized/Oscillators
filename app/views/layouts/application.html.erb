<!DOCTYPE html>
<html>
  <head>
    <title>Hseadc2016React</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag    'oscillators_pack' %>
  </head>

  <body data-props="<%= Oscillator.all.as_json(only: [:id, :title, :frequency, :wave]).to_json %>">
    <%= yield %>
  </body>

</html>
